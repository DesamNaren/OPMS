<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="exitdialog_animation1">
        <item name="android:windowEnterAnimation">@anim/exitdialog_anim1</item>
        <item name="android:windowExitAnimation">@anim/exitdialog_anim2</item>
    </style>

    <style name="roundedDialog">
        <item name="android:background">@drawable/btn_background_green</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

    <style name="roundedDialog_print">
        <item name="android:background">@drawable/btn_background_gray</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    <style name="roundedDialog_red">
        <item name="android:background">@drawable/rounded_corner_button_red</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    <style name="roundedDialog_red_register">
        <item name="android:background">@drawable/btn_background_cancel</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    <style name="roundedDialog_yellow">
        <item name="android:background">@drawable/btn_background_yellow</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    <style name="roundedDialogInfo">
        <item name="android:background">@drawable/btn_background1</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    <style name="circleBackgroundInfo">
        <item name="android:background">@drawable/dialog_circle</item>
    </style>
    <style name="circleBackground">
        <item name="android:background">@drawable/dialog_circle_green</item>
    </style>
    <style name="circleBackground_red">
        <item name="android:background">@drawable/dialog_circle_red</item>
    </style>
    <style name="circleBackground_yellow">
        <item name="android:background">@drawable/dialog_circle_yellow</item>
    </style>
</resources>
