<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/FR_LandDetailsMain_Tr"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="2dp"
    android:orientation="horizontal"
    android:padding="2dp">


    <CheckBox
        android:visibility="gone"
        android:id="@+id/FRLd_check_Cb"
        android:layout_width="60dp"
        android:layout_height="fill_parent"
        android:gravity="center_vertical"
        android:padding="5dp" />

    <ImageView
        android:visibility="gone"
        app:srcCompat="@android:drawable/ic_menu_delete"
        android:id="@+id/FRLd_delete"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:padding="5dp" />

    <Spinner
        android:id="@+id/FRLd_landEnjoyerNature_Sp"
        android:layout_width="150dp"
        android:layout_height="fill_parent"
        android:layout_marginLeft="2dp"
        android:background="@drawable/rounded_corner_et_disable"
        android:entries="@array/farmer_natureT"
        android:textSize="20sp" />

    <LinearLayout
        android:id="@+id/FRLd_landDetails_LL"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:visibility="visible">

        <EditText
            android:id="@+id/FRLd_ownerName_Et"
            android:layout_width="200dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical"
            android:hint="@string/OwnerName"
            android:imeOptions="actionNext"
            android:inputType="text"
            android:maxLength="75"
            android:padding="5dp"
            android:scrollHorizontally="true"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/FRLd_ownerAadhaar_Et"
            android:layout_width="150dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical"
            android:hint="@string/OwnerAadhaar"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:maxLength="12"
            android:padding="5dp"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <Spinner
            android:id="@+id/FRLd_relationship_Sp"
            android:layout_width="150dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:entries="@array/relation_types"
            android:gravity="center_vertical" />

        <Spinner
            android:id="@+id/FRLd_district_Sp"
            android:layout_width="155dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical" />

        <Spinner
            android:id="@+id/FRLd_mandal_Sp"
            android:layout_width="155dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical" />

        <Spinner
            android:id="@+id/FRLd_village_Sp"
            android:layout_width="155dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical" />

        <EditText
            android:id="@+id/FRLd_surveyNo_Et"
            android:layout_width="200dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical"
            android:hint="@string/SurveyNo"
            android:imeOptions="actionNext"
            android:inputType="text"
            android:maxLength="30"
            android:padding="5dp"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/FRLd_khattaNo_Et"
            android:layout_width="220dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical"
            android:hint="@string/Passbookno"
            android:imeOptions="actionNext"
            android:inputType="text"
            android:maxLength="30"
            android:padding="5dp"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/FRLd_landAcers_Et"
            android:layout_width="115dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical"
            android:hint="@string/Landinacers"
            android:imeOptions="actionDone"
            android:inputType="numberDecimal"
            android:maxLength="10"
            android:padding="5dp"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <Spinner
            android:id="@+id/FRLd_landType_Sp"
            android:layout_width="115dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:entries="@array/land_type"
            android:gravity="center_vertical" />

        <EditText
            android:id="@+id/Already_FRLd_LeasedExtent_Et"
            android:layout_width="115dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical"
            android:hint="@string/Landinacers"
            android:imeOptions="actionNext"
            android:inputType="numberDecimal"
            android:maxLength="10"
            android:padding="5dp"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />


        <EditText
            android:id="@+id/FRLd_LeasedExtent_Et"
            android:layout_width="115dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et"
            android:gravity="center_vertical"
            android:hint="@string/Landinacers"
            android:imeOptions="actionNext"
            android:inputType="numberDecimal"
            android:maxLength="10"
            android:padding="5dp"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />


        <EditText
            android:id="@+id/Already_FRLd_cultivatedLand_Et"
            android:layout_width="115dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical"
            android:hint="@string/Landinacers"
            android:imeOptions="actionDone"
            android:inputType="numberDecimal"
            android:maxLength="10"
            android:padding="5dp"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/FRLd_cultivatedLand_Et"
            android:layout_width="115dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et"
            android:gravity="center_vertical"
            android:hint="@string/Landinacers"
            android:imeOptions="actionDone"
            android:inputType="numberDecimal"
            android:maxLength="10"
            android:padding="5dp"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />
        <!--<TextView-->
            <!--android:id="@+id/yeald_Per_Accer"-->
            <!--android:layout_width="115dp"-->
            <!--android:layout_height="fill_parent"-->
            <!--android:layout_marginLeft="2dp"-->
            <!--android:background="@drawable/rounded_corner_et"-->
            <!--android:gravity="center_vertical"-->
            <!--android:imeOptions="actionNext"-->
            <!--android:inputType="text"-->
            <!--android:hint="@string/PaddyQuantity"-->
            <!--android:maxLength="75"-->
            <!--android:padding="5dp"-->
            <!--android:scrollHorizontally="true"-->
            <!--android:singleLine="true"-->
            <!--android:textColor="@color/black"-->
            <!--android:textStyle="bold" />-->
        <EditText
            android:id="@+id/yeald_Per_Accer"
            android:layout_width="115dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical"
            android:hint="@string/OwnerName"
            android:imeOptions="actionNext"
            android:inputType="text"
            android:maxLength="75"
            android:padding="5dp"
            android:scrollHorizontally="true"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/hiddenID"
            android:layout_width="200dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:background="@drawable/rounded_corner_et_disable"
            android:gravity="center_vertical"
            android:hint="@string/DataSrc"
            android:maxLength="75"
            android:padding="5dp"
            android:scrollHorizontally="true"
            android:singleLine="true"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="gone" />

    </LinearLayout>

</TableRow>